    <DataSet Name="SEL_Subprojects">
      <Query>
        <DataSourceName>Redmine</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@in_project_id">
            <Value>=Parameters!in_project_id.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@in_project_recursion_flag">
            <Value>=Parameters!in_project_recursion_flag.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
-- DECLARE  @in_project_id int 
-- SET @in_project_id = 6


-- DECLARE  @in_project_recursion_flag int 
-- SET @in_project_recursion_flag = 1
-- 	-- nur project:  1; nur children: 2; proj &amp; child: 3


SELECT 
	 Child.id 
	,Child."name" 
	,Child.lft 
	,Child.rgt 
FROM projects AS Parent 

LEFT JOIN projects AS Child 
	ON 
	(
		(
			Child.lft &gt;= Parent.lft -- include &gt; vs. &gt;= to include parent-project 
			AND 
			Child.rgt &lt;= Parent.rgt --include &lt; vs. &lt;= to include parent-project 
		)
	)

WHERE (1=1) 
AND 
(
	Parent.id = @in_project_id 
	OR  
	@in_project_id = -1
)
AND 
(
	(
		@in_project_id = -1 -- -1 = All 
		AND 
		(
			(
				-- this is only for direct children 
				-- (flag &amp; flagtype.AB) != 0)
				CASE WHEN Child.parent_id IS NULL THEN 1 ELSE 2 END -- AS flag 
				&amp; @in_project_recursion_flag 
			) &lt;&gt; 0 
		)
	) 
	OR 
	( 
		@in_project_id &lt;&gt; -1
		AND
		(
			-- (flag &amp; flagtype.AB) != 0)
			CASE WHEN Child.id = @in_project_id THEN 1 ELSE 2 END -- AS flag 
			&amp; @in_project_recursion_flag 
		) &lt;&gt; 0 
	)
	
) 

GROUP BY 
	 Child.id 
	,Child."name" 
	,Child.lft 
	,Child.rgt 

ORDER BY 
	 lft 
	,rgt 
	,"name" 
</CommandText>
        <Timeout>300</Timeout>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="lft">
          <DataField>lft</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="rgt">
          <DataField>rgt</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SEL_SubprojectFlags">
      <Query>
        <DataSourceName>Redmine</DataSourceName>
        <CommandText>
      SELECT 1 AS id, N'Nur Projekt'AS name, 1 AS sort 
UNION SELECT 2 AS id, N'Nur Unterprojekte (rekursiv)' AS name, 3 AS sort 
UNION SELECT 3 AS id, N'Projekt + Unterprojekte' AS name, 3 AS sort 
ORDER BY 
	 sort 
	,name 
	 </CommandText>
        <Timeout>300</Timeout>
      </Query>
