    <DataSet Name="SEL_Project">
      <Query>
        <DataSourceName>Redmine</DataSourceName>
        <CommandText>
;WITH CTE AS 
(
	SELECT 
		 projects.id 
		,projects.name
		,projects.parent_id
		,CAST(projects.name AS nvarchar(MAX)) AS recursive_name 
		,ROW_NUMBER() OVER (ORDER BY projects.name) AS sort 
		--,CAST(RIGHT(REPLICATE('0', 20) + CAST(projects.id AS varchar(20)), 20) AS varchar(MAX)) AS recursive_sort 
		,CAST
		(
			RIGHT
			(
				REPLICATE('0', 20) 
				+ 
				CAST
				(
					ROW_NUMBER() OVER (ORDER BY projects.name) 
					AS varchar(20)
				)
				, 20
			) 
			AS varchar(MAX)
		) AS recursive_sort 
		,0 AS Level 
	FROM projects 
	WHERE parent_id IS NULL 

	UNION ALL 

	SELECT 
		 projects.id	
		,projects.name
		,projects.parent_id
		,REPLICATE('    ', CTE.Level + 1) + CTE.recursive_name + N' | ' + CAST(projects.name AS nvarchar(MAX)) AS recursive_name 
		,ROW_NUMBER() OVER (ORDER BY projects.name) AS sort 
		
		--,CAST(CTE.recursive_sort AS varchar(MAX)) + '.' + CAST(RIGHT(REPLICATE('0', 20) + CAST(projects.id AS varchar(20)), 20) AS varchar(MAX)) AS recursive_sort 
		,CAST
		(
			CTE.recursive_sort AS varchar(MAX)) 
			+ '.' 
			+ CAST
			(
				RIGHT
				(
				REPLICATE('0', 20) 
				+ 
				CAST
				(
					ROW_NUMBER() OVER (ORDER BY projects.name) 
					AS varchar(20)
				)
				, 20
			) AS varchar(MAX)
		) AS recursive_sort 
		,CTE.Level + 1 AS Level 
	FROM projects 
	INNER JOIN CTE ON projects.parent_id = CTE.id 
)

SELECT 
	 -1 AS id	
	,'Alle' AS name
	,'Alle' AS recursive_name
	,'_' AS recursive_sort
	,-1 AS Level 

UNION 

SELECT 
	 id	
	,name
	,recursive_name
	,recursive_sort
	,Level 
FROM CTE 
ORDER BY recursive_sort 
</CommandText>
        <Timeout>300</Timeout>
      </Query>
      <Fields>
        <Field Name="id">
          <DataField>id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="recursive_name">
          <DataField>recursive_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="recursive_sort">
          <DataField>recursive_sort</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Level">
          <DataField>Level</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
