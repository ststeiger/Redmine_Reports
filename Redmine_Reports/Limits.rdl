          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="bit">
          <DataField>bit</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
