  <DataSets>
    <DataSet Name="SEL_Limits">
      <Query>
        <DataSourceName>Redmine</DataSourceName>
        <CommandText>
-- https://docs.microsoft.com/en-us/sql/reporting-services/reports/publishing-reports-to-a-report-server

;WITH CTE AS 
(
		  SELECT '-32768' AS "from", '+32767' AS "to", 'Int16' AS "type", 16 AS "bit"
	UNION SELECT '-2147483648' AS "from", '+2147483647' AS "to", 'Int32' AS "type", 32 AS "bit" 
	UNION SELECT '-9223372036854775808' AS "from", '+9223372036854775807' AS "to", 'Int64' AS "type", 65 AS "bit" 
)
SELECT 
	 "type"
	,"from"	
	,"to"
	,LEN("from") AS from_len
	,LEN("to") AS to_len
	,"bit"
FROM CTE 
ORDER BY "bit" </CommandText>
        <Timeout>300</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="type">
          <DataField>type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="from">
          <DataField>from</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="to">
          <DataField>to</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="from_len">
