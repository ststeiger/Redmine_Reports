  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Redmine</DataSourceName>
        <CommandText>;WITH CTE AS 
(
	SELECT 
		 projects.id 
		,projects.name
		,projects.parent_id
		,CAST(projects.name AS nvarchar(MAX)) AS recursive_name 
		,CAST(RIGHT(REPLICATE('0', 20) + CAST(projects.id AS varchar(20)), 20) AS varchar(MAX)) AS recursive_sort 
		,0 AS Level 
	FROM projects 
	WHERE parent_id IS NULL 

	UNION ALL 

	SELECT 
		 projects.id	
		,projects.name
		,projects.parent_id
		,REPLICATE('    ', CTE.Level + 1) + CTE.recursive_name + N' | ' + CAST(projects.name AS nvarchar(MAX)) AS recursive_name 
		,CAST(CTE.recursive_sort AS varchar(MAX)) + '.' + CAST(RIGHT(REPLICATE('0', 20) + CAST(projects.id AS varchar(20)), 20) AS varchar(MAX)) AS recursive_sort 
		,CTE.Level + 1 AS Level 
	FROM projects 
	INNER JOIN CTE ON projects.parent_id = CTE.id 
)
SELECT 
	 id	
	,name
	,recursive_name
	,recursive_sort
	,Level 
FROM CTE 
ORDER BY recursive_sort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
